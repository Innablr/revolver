{
  "name": "revolver",
  "version": "0.9.0",
  "description": "Multipurpose AWS idler script",
  "main": "invoke.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "mocha",
    "bundle": "esbuild revolver.ts --bundle --platform=node --target=node18 --minify --outfile=bundle.js",
    "clean": "rimraf bundle.js",
    "start": "CONFIG_FILE=revolver-config.yaml node invoke.js"
  },
  "author": "Alex Bukharov",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1472.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.126",
    "@types/node": "^20.9.0",
    "chai": "^4.3.10",
    "esbuild": "^0.19.4",
    "eslint": "^8.51.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^10.2.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
