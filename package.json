{
  "name": "revolver",
  "version": "1.0.0",
  "description": "Multipurpose AWS idler script",
  "main": "invoke.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "mocha --require ts-node/register --require source-map-support/register --recursive --exit test",
    "build": "tsc",
    "tsc-watch": "tsc --watch",
    "bundle": "zip -9rq revolver.zip node_modules drivers lib plugins revolver.js",
    "clean": "rimraf revolver.zip",
    "start": "CONFIG_FILE=revolver-config.yaml node invoke.js"
  },
  "author": "Alex Bukharov",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1472.0",
    "deepmerge-ts": "^5.1.0",
    "js-yaml": "^4.1.0",
    "luxon": "^3.4.4",
    "proxy-agent": "^6.3.1",
    "tslog": "^4.9.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.131",
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/luxon": "^3.4.1",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "esbuild": "^0.19.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "mocha": "^10.2.0",
    "prettier": "^3.2.2",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
