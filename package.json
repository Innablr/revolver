{
  "name": "revolver",
  "version": "2.0.0",
  "description": "Multipurpose AWS idler script",
  "main": "invoke.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "test": "mocha --require ts-node/register --require source-map-support/register --recursive --exit test",
    "build": "tsc --build ./tsconfig.json",
    "watch": "tsc --build ./tsconfig.json --watch",
    "prep": "rimraf -g 'plugins/**/**.d.ts' 'plugins/**/*.js' 'drivers/**/*.d.ts' 'drivers/**/*.js' 'lib/**/*.d.ts' 'lib/**/*.js' index.d.ts index.js revolver.d.ts revolver.js",
    "bundle": "rimraf dist; npm run prep; webpack; zip -9rqj revolver.zip dist",
    "clean": "npm run prep; tsc --build ./tsconfig.json --clean && rimraf revolver.zip dist;",
    "start": "CONFIG_FILE=revolver-config.yaml node invoke.js"
  },
  "author": "Alex Bukharov",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-auto-scaling": "^3.501.0",
    "@aws-sdk/client-ec2": "^3.501.0",
    "@aws-sdk/client-organizations": "^3.504.0",
    "@aws-sdk/client-rds": "^3.501.0",
    "@aws-sdk/client-redshift": "^3.501.0",
    "@aws-sdk/client-s3": "^3.501.0",
    "@aws-sdk/client-sts": "^3.501.0",
    "@aws-sdk/credential-providers": "^3.501.0",
    "@aws-sdk/types": "^3.496.0",
    "@smithy/node-http-handler": "^2.3.1",
    "https-proxy-agent": "^7.0.2",
    "js-yaml": "^4.1.0",
    "jsonfile": "^6.1.0",
    "luxon": "^3.4.4",
    "proxy-agent": "^6.3.1",
    "ts-deepmerge": "^7.0.0",
    "tslog": "^4.9.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.132",
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonfile": "^6.1.4",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.7",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "mocha": "^10.2.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  }
}
