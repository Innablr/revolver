{
  "name": "revolver",
  "version": "2.0.0",
  "description": "Multipurpose AWS idler script",
  "main": "invoke.ts",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "test": "mocha --require ts-node/register --require source-map-support/register --recursive --exit test",
    "coverage": "nyc npm run test; rimraf .nyc_output",
    "build": "tsc --build ./tsconfig.json",
    "watch": "tsc --build ./tsconfig.json --watch",
    "bundle": "npm run clean; webpack; npm run build; zip -9rqj revolver.zip dist",
    "bundleTestPrep": "mkdir dist; cp invoke.js dist; cp test/bundle-test-config.yaml dist/bundle-test-config.yaml; cp resources.json dist/bundle-test-resources.json",
    "bundleTestRun": "cd dist; CONFIG_FILE=bundle-test-config.yaml node invoke.js",
    "bundleTest": "npm run bundle; npm run bundleTestPrep; npm run bundleTestRun",
    "cleanIntermediate": "rimraf -g 'plugins/**/**.d.ts' 'plugins/**/*.js' 'drivers/**/*.d.ts' 'drivers/**/*.js' 'lib/**/*.d.ts' 'lib/**/*.js' invoke.d.ts invoke.js revolver.d.ts revolver.js",
    "clean": "npm run cleanIntermediate; tsc --build ./tsconfig.json --clean && rimraf revolver.zip dist;",
    "start": "CONFIG_FILE=revolver-config.yaml PRETTY_LOG_TIME_ZONE=local node invoke.js"
  },
  "author": "Alex Bukharov",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-auto-scaling": "^3.549.0",
    "@aws-sdk/client-ec2": "^3.549.0",
    "@aws-sdk/client-organizations": "^3.549.0",
    "@aws-sdk/client-rds": "^3.549.0",
    "@aws-sdk/client-redshift": "^3.549.0",
    "@aws-sdk/client-s3": "^3.550.0",
    "@aws-sdk/client-sqs": "^3.549.0",
    "@aws-sdk/client-sts": "^3.511.0",
    "@aws-sdk/credential-providers": "^3.549.0",
    "@aws-sdk/types": "^3.511.0",
    "@smithy/node-http-handler": "^2.5.0",
    "https-proxy-agent": "^7.0.4",
    "jmespath": "^0.16.0",
    "js-yaml": "^4.1.0",
    "jsonfile": "^6.1.0",
    "luxon": "^3.4.4",
    "proxy-agent": "^6.4.0",
    "ts-deepmerge": "^7.0.0",
    "tslog": "^4.9.2",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.137",
    "@types/chai": "^4.3.14",
    "@types/chai-as-promised": "^7.1.8",
    "@types/jest": "^29.5.12",
    "@types/jmespath": "^0.15.2",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonfile": "^6.1.4",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^12.0.2",
    "csv-parse": "^5.5.5",
    "eslint": "^8.57.0 <9",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-tsdoc": "^0.2.17",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
