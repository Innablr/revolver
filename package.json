{
  "name": "revolver",
  "version": "1.0.0",
  "description": "Multipurpose AWS idler script",
  "main": "invoke.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "mocha",
    "build": "tsc",
    "bundle": "zip -9rq revolver.zip node_modules drivers lib plugins revolver.js",
    "clean": "rimraf revolver.zip",
    "start": "CONFIG_FILE=revolver-config.yaml node invoke.js"
  },
  "author": "Alex Bukharov",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1472.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.126",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "chai": "^4.3.10",
    "esbuild": "^0.19.4",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "mocha": "^10.2.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
