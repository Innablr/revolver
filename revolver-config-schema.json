                        },
                        "additionalProperties": false
                      },
                      "not": {
                        "$ref": "#/properties/defaults/properties/settings/properties/excludeResources/anyOf/0/items"
                      },
                      "or": {
                        "type": "array",
                        "items": {
                          "$ref": "#/properties/defaults/properties/settings/properties/excludeResources/anyOf/0/items"
                        }
                      },
                      "and": {
                        "type": "array",
                        "items": {
                          "$ref": "#/properties/defaults/properties/settings/properties/excludeResources/anyOf/0/items"
