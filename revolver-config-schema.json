                              },
                              "contains": {
                                "$ref": "#/properties/defaults/properties/settings/properties/excludeResources/anyOf/0/items/properties/tag/anyOf/2/properties/contains"
                              },
                              "startswith": {
                                "$ref": "#/properties/defaults/properties/settings/properties/excludeResources/anyOf/0/items/properties/tag/anyOf/2/properties/startswith"
                              },
                              "endswith": {
                                "$ref": "#/properties/defaults/properties/settings/properties/excludeResources/anyOf/0/items/properties/tag/anyOf/2/properties/endswith"
                              },
                              "regexp": {
                                "$ref": "#/properties/defaults/properties/settings/properties/excludeResources/anyOf/0/items/properties/tag/anyOf/2/properties/regexp"
                              }
                            },
                            "required": [
                              "path"
