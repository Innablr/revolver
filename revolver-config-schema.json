                            "items": {
                              "$ref": "#/properties/defaults/properties/settings/properties/excludeResources/anyOf/0/items/properties/tag/anyOf/0/items"
                            }
                          },
                          {
                            "$ref": "#/properties/defaults/properties/settings/properties/excludeResources/anyOf/0/items/properties/tag/anyOf/0/items"
