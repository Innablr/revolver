              "properties": {
                "json": {
                  "type": "object",
                  "properties": {
                    "overwrite": {
                      "type": "boolean",
                      "default": true
                    },
                    "file": {
                      "type": "string"
                    },
                    "sqs": {
                      "type": "object",
                      "properties": {
                        "url": {
                          "type": "string",
                          "format": "uri"
                        },
                        "compress": {
                          "type": "boolean",
                          "default": true
                        },
                        "attributes": {
                          "type": "object",
                          "additionalProperties": {
                            "type": "string"
                          }
                        }
                      },
                      "required": [
                        "url"
                      ],
                      "additionalProperties": false
                    },
                    "sns": {
                      "type": "object",
                      "properties": {
                        "url": {
                          "type": "string",
                          "pattern": "^arn:.*$"
                        },
                        "compress": {
                          "type": "boolean",
                          "default": true
                        },
                        "attributes": {
                          "type": "object",
                          "additionalProperties": {
                            "type": "string"
                          }
                        }
                      },
                      "required": [
                        "url"
                      ],
                      "additionalProperties": false
                    },
                    "s3": {
                      "type": "object",
                      "properties": {
                        "bucket": {
                          "type": "string",
                          "pattern": "(?!(^((2(5[0-5]|[0-4][0-9])|[01]?[0-9]{1,2})\\.){3}(2(5[0-5]|[0-4][0-9])|[01]?[0-9]{1,2})$|^xn--|.+-s3alias$))^[a-z0-9][a-z0-9.-]{1,61}[a-z0-9]$"
                        },
                        "region": {
                          "$ref": "#/properties/defaults/properties/settings/properties/region"
                        },
                        "path": {
                          "type": "string"
                        }
                      },
                      "required": [
                        "bucket",
                        "region",
                        "path"
                      ],
                      "additionalProperties": false
                    }
                  },
                  "additionalProperties": false
                },
                "html": {
                  "type": "object",
                  "properties": {
                    "file": {
                      "$ref": "#/properties/defaults/properties/settings/properties/resourceLog/properties/json/properties/file"
                    },
                    "sqs": {
                      "$ref": "#/properties/defaults/properties/settings/properties/resourceLog/properties/json/properties/sqs"
                    },
                    "sns": {
                      "$ref": "#/properties/defaults/properties/settings/properties/resourceLog/properties/json/properties/sns"
                    },
                    "s3": {
                      "$ref": "#/properties/defaults/properties/settings/properties/resourceLog/properties/json/properties/s3"
                    }
                  },
                  "additionalProperties": false
                },
                "csv": {
                  "type": "object",
