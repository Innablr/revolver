                    "file": {
                      "$ref": "#/properties/defaults/properties/settings/properties/resourceLog/properties/json/properties/file"
                    },
                    "sqs": {
                      "$ref": "#/properties/defaults/properties/settings/properties/resourceLog/properties/json/properties/sqs"
                    },
                    "sns": {
                      "$ref": "#/properties/defaults/properties/settings/properties/resourceLog/properties/json/properties/sns"
                    },
                    "s3": {
                      "$ref": "#/properties/defaults/properties/settings/properties/resourceLog/properties/json/properties/s3"
                    }
                  },
                  "additionalProperties": false
                },
                "console": {
                  "anyOf": [
                    {
                      "type": "null"
                    },
                    {
                      "type": "object",
                      "properties": {
                        "reportTags": {
                          "type": "array",
