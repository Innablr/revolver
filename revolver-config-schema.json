              "pattern": "^([A-Za-z]+\\/[A-Za-z_]+|UTC(?:[+-]\\d+)?)$",
              "default": "UTC"
            },
            "timezoneTag": {
              "type": "string",
              "default": "Timezone"
            },
            "concurrency": {
              "type": "number",
              "default": 0
            },
            "organizationRoleName": {
              "type": "string"
            },
            "revolverRoleName": {
              "type": "string"
            },
